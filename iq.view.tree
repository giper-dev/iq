$giper_iq $mol_book2
	ranks *
		XXL @ \ğŸ˜ Mega Mind
		XL @ \ğŸ˜ Smart Ass
		L @ \ğŸ˜Š Too Clever
		M @ \ğŸ˜ Just Normie
		S @ \ğŸ˜® So Stupid
		XS @ \ğŸ¥´ Jack Ass
		XXS @ \ğŸ¤ª Brain Less
	required 200
	history?val /number
	auto /
		<= Brain $giper_iq_neuron<number> / 0
		<= score 0
	plugins /
		<= Theme $mol_theme_auto
		<= Hotkey $mol_hotkey
			key *
				left?event <=> left?event
				right?event <=> right?event
	placeholders /
	pages /
		<= Space $giper_web_frame
		<= Main $mol_page
			title <= title @ \Giper IQ
			Title => Main_title
			Tools => Main_tools
			head /
				<= Caption $mol_view sub /
					<= Share $mol_button_share
					<= Main_title
				<= Score $mol_chip sub / <= score
				<= Main_tools
			tools /
				<= Sources $mol_link_source
					uri \https://github.com/giper-dev/iq/
				<= Donate $mol_link_donate
					uri \https://boosty.to/hyoo
				<= Support $mol_link_support
					uri \https://t.me/giper_web/137
				<= Lights $mol_lights_toggle
			Body_content => Main_body_content
			body_content /
				<= Score_final $mol_paragraph sub / <= score_final \
				<= Main_body_content
			body /
				<= Stats $mol_chart
					Legend null
					graphs /
						<= Score_log $mol_plot_group
							graphs /
								<= Score_line $mol_plot_line
								<= Score_fill $mol_plot_fill
							series_y <= score_series?val /number
						<= Bad $mol_plot_fill
							series_x /number
								+0
								+100
							series_y /number
								-100
								-100
						<= Good $mol_plot_fill
							series_x /number
								+0
								+100
							series_y /number
								+100
								+100
						<= Ruler_vert $mol_plot_ruler_vert
							title \
						<= Ruler_hor $mol_plot_ruler_hor
							title \
				<= History_log $mol_scroll
					sub /
						<= description @ \Click buttons to maximize your score!
				<= Choices $mol_row
					sub /
						<= Left $mol_button_major
							title <= left_title @ \Left
							click?event <=> left?event null
						<= Right $mol_button_major
							title <= right_title @ \Right
							click?event <=> right?event null
